<h3>Add a new Appointment</h3>
<%= form_for @appointment do |f| %>
  <%= f.label :date, "Enter Date (yyyy/mm/dd)" %>
  <%= f.text_field :date %><br/>
  <%= f.label :street, "Enter Street Number" %>
  <%= f.text_field :street %><br/>
  <%= f.label :city, "Enter City" %>
  <%= f.text_field :city %><br/>
  <%= f.label :zip, "Enter 5 Digit Zip Code" %>
  <%= f.text_field :zip %><br/>
  <%= f.submit "Add Appointment" %>
<% end %>